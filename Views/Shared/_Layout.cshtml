	<script>
		var accessiblePages = [];
		$(function () {
			var role = '@Context.Request.HttpContext.Session.GetString("role")';
			if (role) {
				$.ajax({
					url: '/PageAccess/GetAccessiblePages',
					type: 'GET',
					data: { role: role },
					success: function (data) {
						accessiblePages = data;
						updateSidebar();
					}
				});
			}
		});
		function updateSidebar() {
			$('.sidebar .dropdown-item').each(function () {
				var id = $(this).attr('id');
				if (accessiblePages.indexOf(id) > -1) {
					$(this).show();
				} else {
					$(this).hide();
				}
			});
		}
	</script>



